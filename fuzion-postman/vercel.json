{
  "name": "fuzion-postman",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "@api_base_url",
    "NEXT_PUBLIC_COLLECTIONS_URL": "@collections_url",
    "NEXT_PUBLIC_BULK_UPDATE_URL": "@bulk_update_url",
    "NEXT_PUBLIC_SAVE_API_ENDPOINT": "/api/apis/create",
    "NEXT_PUBLIC_API_DETAILS_ENDPOINT": "/api/apis/key",
    "POSTMAN_PORT": "4002"
  }
}